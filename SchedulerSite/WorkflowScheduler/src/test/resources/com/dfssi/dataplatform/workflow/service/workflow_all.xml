            </configuration>
            <main-class>com.boco.BSSystem.impala.ImpalaCollect</main-class>
            <arg>DW_F_XDR_PS_GN_DNS_CELL_H</arg>
            <arg>${day}</arg>
            <arg>${hour}</arg>
        </java>
        <ok to="ODS_RE_ST_XDR_PS_CAUSE_DNS_H" />
        <error to="fail" />
