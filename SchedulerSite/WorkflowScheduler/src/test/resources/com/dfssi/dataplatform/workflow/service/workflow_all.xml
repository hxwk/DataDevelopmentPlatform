            <arg>${hour}</arg>
        </java>
        <ok to="DEL_ODS_RE_ST_XDR_PS_CAUSE_DNS_H" />
        <error to="fail" />
    </action>

    <!-- 调用oracle存储过程删除小时数据 -->
